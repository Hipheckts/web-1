{
    "logging": {
        "enabled": true,
        "level": "DEBUG",
        "path": "./log",
        "filename": "rosecomb",
        "dateFormat": "",
        "extension": "log",
        "messageFormat": "<%= label %> - <%= date %> - <%= message %>"
    },
    "caching": {
        "enabled": false,
        "ttl": 300,
        "directory": "./cache/rosecomb/",
        "extension": "html"
    },
    "server": {
        "host": "127.0.0.1",
        "port": 3001
    },
    "api": {
        "host": "127.0.0.1",
        "port": 3000
    },
    "auth": {
        "tokenUrl":"/token",
        "clientId":"testClient",
        "secret":"superSecret"
    },
    "global" : {
        "baseUrl": "http://www.example.com"
    },
    "dust": {
        "cache": false,
        "debug": true,
        "debugLevel": "DEBUG",
        "whitespace": true 
    }
}
