{
    "logging": {
        "enabled": true,
        "level": "DEBUG",
        "path": "./log",
        "filename": "rosecomb",
        "dateFormat": "YYYY-MM-DD",
        "extension": "log",
        "messageFormat": "<%= label %> - <%= date %> - <%= message %>"
    },
    "caching": {
        "enabled": true,
        "ttl": 300,
        "directory": "./cache/rosecomb/",
        "extension": "html"
    },
    "server": {
        "host": "localhost",
        "port": 3001
    },
    "api": {
        "host": "localhost",
        "port": 3000
    },
    "auth": {
        "tokenUrl":"/token",
        "clientId":"rosecombClient",
        "secret":"superSecret"
    },
    "global" : {
        "baseUrl": "http://www.example.com"
    },
    "dust": {
        "cache": false,
        "debug": true,
        "debugLevel": "DEBUG",
        "whitespace": true 
    },
    "feedback": false,
    "useSlackIntegration": false
}
