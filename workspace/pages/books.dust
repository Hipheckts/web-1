{>layout/}

{<content}

  <h3>Books ({@size key=books/} records)</h3>
  <ul>
  {#books}
    <li>
      <p>{name}, {byline} by {authorId}</p>
    </li>
  {/books}
  </ul>

  <h3>Users ({@size key=users/} records)</h3>
